.wxj_Logos_shows
  width 100%
  height 692px
  background #f7f7f7
  padding-top 50px

.wxj_show_contain
  width 1180px
  height 592px
  margin 0 auto
  padding-top 30px

.wxj_Logos_text_another
  background url('https://muse.huaban.com/assets/1921757c61d97713fff81352b1295c1c.svg') no-repeat -92px -102px
  width 322px
  height 23px
  font-size 24px
  font-weight 100
  color #f7f7f7
  margin-left auto
  margin-right auto
  letter-spacing 7px

.wxj_Logos_line
  width 100px
  height 2px
  background-color #c0a75e
  margin 30px auto
  color #f8f8f8

.wxj_show_picture
  width 194px
  height 157px
  background white
  margin-bottom 1px
  margin-top 1px
  position relative
  margin-left 2.6px
  float left
  overflow hidden
  text-decoration none

.wxj_show_picture:hover
  transform scale(1.02,1.05)
  margin-left 1px
  margin-right 1px
  margin-bottom 1px
  transition box-shadow 0.2s ease-out,transform 0.2s ease-in-out
  background-color white
  box-shadow 2px 1px 20px 1px lightgrey

.wxj_show_picture>img
  width 70px
  height 70px
  position absolute
  top 15px
  left 60px

.wxj_show_text
  width 154px
  height 19px
  position absolute
  font-size 14px
  font-weight 200
  text-align center
  left 20px
  top 100px
  letter-spacing 3px

.wxj_show_bottom
  width 1180px
  height 68px
  margin auto
  margin-top 40px
  float left

.wxj_Logos_find
  width 200px
  height 22px
  font-size 16px
  letter-spacing 4px
  color #999
  text-align center
  margin auto

.wxj_Logos_btn
  width 144px
  height 40pxline
  color #222
  border 1px solid black
  text-align center
  line-height 40px
  margin auto
  margin-top 20px


.wxj_Logos_btn:hover
  background black
  color white