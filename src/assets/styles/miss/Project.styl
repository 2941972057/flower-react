.wxj_Project
  width 100%
  height 730px
  background #f7f7f7
  padding-top 40px

.wxj_Project_another1
  background url('https://muse.huaban.com/assets/1921757c61d97713fff81352b1295c1c.svg') no-repeat -167px -401px
  width 171px
  height 23px
  font-size 24px
  font-weight 100
  margin auto
  margin-top 10px
  color #f7f7f7
  letter-spacing 7px

.wxj_Project_contain
  width 100%
  height 540px
  margin 40px auto
  position relative

.wxj_Project_block
  width 1170px
  height 540px
  margin auto
  overflow hidden
  position relative

.wxj_Project_content
  width 3564px
  height 540px
  position absolute
  left 0
  background-color white
  transition all 1s

.wxj__Project_up
  position relative
  left 0
  width 3564px
  height 380px
  display block
  float left

.wxj__Project_down
  width 380px
  height 160px
  float left

.wxj_Project_pic
  width 380px
  background white
  float left
  margin-top 5px
  margin-right 10px
  display block
  //margin-left 20px
  transition box-shadow .2s ease-in-out,transform .2

.wxj_Project_img:hover
  margin-top -3px
  transition all 0.1s

.wxj_Project_img
  width 380px
  height 540px
  float left
  background-color: white;
  margin-right: 16px;
  text-decoration none

.wxj_Project_img>img
  width 380px
  height 380px
  text-decoration none
  float left


.wxj_Project_text1
  width 380px
  height 19px
  color #222
  float left
  font-size 16px
  font-weight 700
  text-decoration none
  letter-spacing 3px
  text-align center

.wxj_Logos_line
  width 100px
  height 2px
  background-color #c0a75e
  margin 30px auto
  color #f8f8f8

.wxj_Project_text2
  width 380px
  height 19px
  float left
  margin-top 14px
  letter-spacing 3px
  font-size 14px
  color #999
  text-align center

.wxj_Project_text3
  width 380px
  height 19px
  float left
  margin-top 10px
  font-size 14px
  color #c0a75e
  text-align center

.wxj_Project_text4
  width 380px
  height 19px
  float left
  margin-top 10px
  font-size 14px
  color #c0a75e
  text-align center

.wxj_Project_dot
  width 60px
  height 20px
  margin 0 auto

.wxj_Project_circle
  width 15px
  height 15px
  border-radius 10px
  border 1px solid lightgrey
  color #f7f7f7
  float left
  background-color white
  margin-right 3px
  cursor pointer

 .wxj_Project_points
  width 1200px
  height 540px
  position absolute
  left 0
  top 0

.wxj_Project_point1
  margin-top 190px
  background url('https://muse.huaban.com/assets/91dbc8b904f51239562fbca1992dfc66.svg') no-repeat -1px -1px
  width 33px
  height 63px
  position absolute
  color #f7f7f7

.wxj_Project_point1:hover
  background url('https://muse.huaban.com/assets/91dbc8b904f51239562fbca1992dfc66.svg') no-repeat -1px -81px
  width 33px
  height 63px
  color #f7f7f7


