#header-loginList
  width 100%
  height 120px
  background url("../../../images/activity/login_header_blurbg.jpg")
#loginList
  width 1496px
  height 120px
  margin auto
@media screen and (max-width: 1288px)
  #loginList
    width 992px
@media screen and (max-width: 1542px) and (min-width: 1289px)
  #loginList
    width 1244px
#loginList::after
  content ''
  display block
  clear both
#loginList-left
  float left
  width 380px
  margin-top 36px
  height 58px
  color white
#loginList-title
  font-size 22px
  margin-bottom 8px
#loginList-right
  float right
  width 261px
  position relative
  top 20px
.qq
  display inline-block
  width: 54px
  height: 54px
  border-radius 50%
  background-color #FBF6F3
  margin-left 15px
.dou
  display inline-block
  width: 54px
  height: 54px
  border-radius 50%
  background-color #FBF6F3
  margin-left 15px
.ren
  display inline-block
  width: 54px
  height: 54px
  border-radius 50%
  background-color #FBF6F3
  margin-left 15px
.img
  height: 54px
  background url("../../../images/activity/微博 (1).png") no-repeat 5px center
  background-size: 100%;
  width: 80%
.img1
  height: 54px
  background url("../../../images/activity/qq.png") no-repeat 10px center
  background-size: 77%;
  width:80%
.img2
  height: 54px
