#wrapper-box
  width 100%
  background-color white
#wrapper
  width 1496px
  height 170px
  text-align center
  padding-top 45px
  box-sizing border-box
  margin auto
@media screen and (max-width: 1288px)
  #wrapper
    width 992px

@media screen and (max-width: 1542px) and (min-width: 1289px)
  #wrapper
    width 1244px

#wrapper-title-box>div
  display inline-block
#wrapper-title
  font-size 45px
  font-weight lighter
#wrapper-title-box
  width 410px
  height 50px
  margin auto
  margin-bottom 30px
.wrapper-line
  position relative
  top -18px
  width 50px
  height 2px
  margin 0 20px
  border-bottom 1px solid black
#navs
  height 45px
#navs>a
  display inline-block
  padding 12px 10px
  margin 0 10px
  color black
  text-decoration none
.theLine
  border-bottom 1px solid black